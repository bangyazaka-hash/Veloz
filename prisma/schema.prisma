generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model admin {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String @db.VarChar(255)
}

model Frame {
  id        Int    @id @default(autoincrement())
  judul     String
  harga     Int
  deskripsi String
  gambar    String
}

model Wheel {
  id        Int    @id @default(autoincrement())
  judul     String
  harga     Int
  deskripsi String
  gambar    String
}

model Component {
  id        Int    @id @default(autoincrement())
  judul     String
  harga     Int
  deskripsi String
  gambar    String
}

